package com.test;

import java.util.ArrayList;
import java.util.List;

public class Test {

	public static void main(String[] args) {
		List<String> list = new ArrayList<String>();
		list.add("abc");
		list.add("zyx");
		System.out.println("list: "+list);
		for(String name: list) {
			if(name.equals("abc")) {
				System.out.println("name:"+ name);
			}
		}
	}

}
